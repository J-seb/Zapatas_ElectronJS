{
  "name": "zapatas-app",
  "version": "1.0.0",
  "productName": "zapatas-app",
  "description": "zapatas_app",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . zapatas-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Zapatas App\"",
    "package-linux": "electron-packager . zapatas-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^11.2.3",
    "electron-packager": "^15.2.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "bootstrap": "^5.0.0-beta2",
    "canvas": "^2.7.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "jspdf": "^2.3.1",
    "jspdf-autotable": "^3.5.14",
    "pdfmake": "^0.1.71",
    "popper.js": "^1.16.1",
    "tooltip.js": "^1.3.3",
    "uuid": "^8.3.2"
  }
}
